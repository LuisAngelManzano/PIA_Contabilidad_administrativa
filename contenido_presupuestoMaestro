print("Presupuesto del 1 de Enero al 31 de Diciembre del 2018")
print(f'Saldo Inicial de Efectivo:\t\t |{efectivo:10}')
print("Entradas:")
print(f'Cobranza 2018:\t {suma_porcentaje:10}')
print(f'Cobranza 2017:\t {clientes:10}')
sumaflujo=suma_porcentaje + clientes
print(f'Total de Entradas:\t\t\t\t | {sumaflujo:10}')
sumatotiaflujo = sumaflujo + efectivo
print(f'Efectivo Disponible:\t\t\t\t | {sumatotiaflujo:10}')
print("Salidas:") #----------------------------------------
print(f'Proveedores 2018: \t | {cobranza_2018:10}')
print(f'Proveedores 2017: \t | {proveedores:10}')
print(f'Pago Mano de Obra Directa: \t | {suma_importe3:10}')
dato =  268000 
print(f'Pago Gastos Indirectos de Fabricación: \t | {dato:10}')
dato2 =  639180 
print(f'Pago de Gastos de Operación: \t | {dato2:10}')
compra_fija =  105000 
print(f'Compra de Activo Fijo (Maquinaria): \t | {compra_fija:10}')
print(f'Pago ISR 2017: \t | {ISR_por_pagar:10}')
total1  = cobranza_2018 + proveedores + suma_importe3 + dato + dato2 + compra_fija + ISR_por_pagar
print(f'Total de Salidas: \t\t\t\t | {total1:10}')
total2 = sumatotiaflujo - total1
print(f'Flujo de Efectivo Actual: \t\t\t\t | {total2:10}')


print(f'{"-"*70}')
print("\t MAQUILADOS AFILIER, S.A.")
