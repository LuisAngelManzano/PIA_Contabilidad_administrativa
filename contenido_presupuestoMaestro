mul_obradirecta2 = unidad_producir2 * horas_mano_obra_b
mul_obradirecta3 = unidad_producirtotal* horas_mano_obra_b
print(f"Total de horas requeridas:\t\t|{mul_obradirecta1:10}\t\t |{mul_obradirecta2:10}\t\t\t| {mul_obradirecta3:10}")
cuota_hora1 = 25
cuota_hora2 = 28
print(f"Cuota por hora:\t\t|{cuota_hora1:10}\t\t |{cuota_hora2:10}\t\t\t| {'':10}")
importe1mul = mul_obradirecta1 * cuota_hora1
importe2mul = mul_obradirecta2 * cuota_hora2
sumaimporte = importe1mul + importe2mul

print(f"Importe de M.O.D.:\t\t|{importe1mul:10}\t\t |{importe2mul:10}\t\t\t| {sumaimporte:10}")
