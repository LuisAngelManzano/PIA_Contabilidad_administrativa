print("  DESCRIPCION\t\t\t\t\t|   Importe\t\t |    Total\t\t|    \t\t")
print(f'Saldo de Proveedores 31-Dic-2017:\t\t\t\t  \t | {proveedores:10}')
print(f'Compras 2018:\t\t\t\t\t\t\t\t | {compra_totales3:10}')
sumatoriacompras = proveedores + compra_totales3
print(f'Total de Proveedores 2018:\t\t\t\t\t\t | {sumatoriacompras:10}')
print("Entradas de Efectivo")
print(f'Por Proveedores del 2017:\t\t\t {proveedores:10}|')
cobranza_2018 =compra_totales3 * 0.5
print(f'Por Proveedores del 2018:\t  \t\t{cobranza_2018:10}|')
suma_cobranza = cobranza_2018 + proveedores
print(f'Total de Salidas 2018::\t\t\t\t \t\t \t | {suma_cobranza:10}')
resta_cobranza = sumatoriacompras - suma_cobranza
print(f'Saldo de Proveedores del 2018:\t\t\t\t \t \t | {resta_cobranza:10}')

